<addons>
<addon id="plugin.program.thumbnails"
       name="Thumbnails"
       version="1.2.0"
       provider-name="Dan Dar3">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
    <import addon="script.module.beautifulsoup" version="3.0.8"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>program</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <minversion>30559</minversion>
    <summary lang="en">Thumbnails</summary>
    <description lang="en">Thumbnails cleaner. Removes cached thumbnails for entries not in your library anymore.</description>
  </extension>
  <statfeed>
    <working>yes</working>
	<notes></notes>
	<maintainer></maintainer>
  </statfeed>
</addon>

<addon id="script.module.xbmcswift" name="xbmcswift" version="0.2.2" provider-name="Jonathan Beluch (jbel)">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.module" library="lib"/>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <source>https://github.com/jbeluch/xbmcswift</source>
  </extension>
</addon>

<addon
  id="plugin.audio.relive"
  version="0.11.0"
  name="reLive"
  provider-name="BuZz">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
     <provides>audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary lang="en">reLive</summary>
    <description lang="en">
      Listen to archives of demoscene related radio shows and podcasts - and primarily scenesat.com - an excellent demo scene orientated radio station - http://www.scenesat.com. It allows you to see all the tracks for a show and skip to a part of your choice. It also features some preset streams for the live broadcasts.
    </description>
    <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
    <email>buzz at exotica org uk</email>
    <website>https://github.com/joolswills/plugin.audio.relive</website>
    <source>https://github.com/joolswills/plugin.audio.relive</source>
  </extension>
    <statfeed>
        <working>yes</working>
        <notes></notes>
        <maintainer>BuZz</maintainer>
    </statfeed>
</addon>

