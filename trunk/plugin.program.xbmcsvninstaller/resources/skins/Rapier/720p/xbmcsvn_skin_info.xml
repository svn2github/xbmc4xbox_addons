<?xml version="1.0" encoding="UTF-8"?>
<window>
    <defaultcontrol>30090</defaultcontrol>
    
	<coordinates>
		<system>1</system>
		<posx>350</posx>
		<posy>60</posy>
	</coordinates>
	
	<controls>
		<!-- ** Background image ** -->
		<control type="image">
			<description>Background Image</description>
			<posx>-49</posx>
			<posy>13</posy>
			<width>680</width>
			<height>580</height>
			<texture>dialog-background.png</texture>
		</control>
		
		<!-- ** Name ** -->
		<control type="label" id="30010">
			<description>Skin name</description>
			<posx>0</posx>
			<posy>50</posy>
			<width>350</width>
			<align>left</align>
			<include>DialogTitle2</include>
		</control>
		
		<!--  ** Rating stars ** -->
		<control type="image" id="30020">
			<description>Rating stars</description>
			<posx>0</posx>
			<posy>80</posy>
			<width>200</width>
			<height>20</height>
		</control>

		<!-- ** Xbox Compatible (label) ** -->
		<control type="label" id="30030">
			<description>Xbox Compatible (label)</description>
			<posx>290</posx>
			<posy>80</posy>
			<width>200</width>
			<align>left</align>
			<font>ListFont2</font>
            <textcolor>FFFFFFFF</textcolor>
		</control>

		<!-- ** Xbox Compatible (value) ** -->
		<control type="label" id="30040">
			<description>xbox Compatible (value)</description>
			<posx>340</posx>
			<posy>80</posy>
			<width>90</width>
			<align>left</align>
			<font>ListFont2</font>
            <textcolor>FFFFFFFF</textcolor>
		</control>

		<!-- ** Downloads (label) ** -->
		<control type="label" id="30050">
			<description>Downloads (label)</description>
			<posx>440</posx>
			<posy>80</posy>
			<width>200</width>
			<align>left</align>
			<font>ListFont2</font>
            <textcolor>FFFFFFFF</textcolor>
		</control>

		<!-- ** Downloads (value) ** -->
		<control type="label" id="30060">
			<description>Downloads (value)</description>
			<posx>580</posx>
			<posy>80</posy>
			<width>90</width>
			<align>right</align>
			<font>ListFont2</font>
            <textcolor>FFFFFFFF</textcolor>
		</control>
		
		<!-- ** Description ** -->
		<control type="textbox" id="30070">
			<description></description>
			<posx>0</posx>
			<posy>105</posy>
			<width>580</width>
			<height>70</height>
			<font>ListFont2</font>
			<textcolor>FFFFFFFF</textcolor>
			<pulseonselect>true</pulseonselect>
		</control>
		
		<control type="group" id="30080">
		
			<!--  ** Previous screenshot (button) -->
			<control type="button" id="30081">
				<description>Screenshot - Previous button</description>
				<posx>5</posx>
				<posy>320</posy>
				<width>32</width>
				<height>32</height>
				<texturefocus>arrow-scroll-left-focus.png</texturefocus>
				<texturenofocus>arrow-scroll-left-nofocus.png</texturenofocus>
				<onleft>30084</onleft>
				<onright>30084</onright>
				<onup>30090</onup>
				<ondown>30090</ondown>
			</control>
			
			<!-- ** Screenshot - Please wait... ** -->
			<control type="label" id="30082">
				<description>Screenshot - please wait...</description>
				<posx>120</posx>
				<posy>325</posy>
				<width>350</width>
				<align>center</align>
				<font>ListFont2</font>
	            <textcolor>FFFFFFFF</textcolor>
			</control>		
			
			<!--  ** Screenshot ** -->
			<control type="image" id="30083">
				<description>Screenshot</description>
				<posx>50</posx>
				<posy>187</posy>
				<width>480</width>
				<height>320</height>
				<visible>true</visible>
				<texture></texture>
				<aspectratio>scale</aspectratio>
			</control>		
			
			<!--  ** Next screenshot (button) -->
			<control type="button" id="30084">
				<description>Screenshot - Next button</description>
				<posx>545</posx>
				<posy>320</posy>
				<width>32</width>
				<height>32</height>
				<texturefocus>arrow-scroll-right-focus.png</texturefocus>
				<texturenofocus>arrow-scroll-right-nofocus.png</texturenofocus>
				<onleft>30081</onleft>
				<onright>30081</onright>
				<onup>30090</onup>
				<ondown>30090</ondown>
			</control>
			
		</control>
		
		<control type="group" id="30090">
		
			<!-- ** Install button ** -->
			<control type="button" id="30091">
				<description>Install Button</description>
				<posx>100</posx>
				<posy>525</posy>
				<include>DialogMenuBarBtn</include>
				<onup>30080</onup>
				<onleft>30092</onleft>
				<onright>30092</onright>
				<ondown>30080</ondown>
			</control>		
	
			<!-- ** Cancel button ** -->
			<control type="button" id="30092">
				<description>Cancel Button</description>
				<posx>310</posx>
				<posy>525</posy>
				<include>DialogMenuBarBtn</include>
				<onup>30080</onup>
				<onleft>30091</onleft>
				<onright>30091</onright>
				<ondown>30080</ondown>
			</control>
			
		</control>
	</controls>
</window>