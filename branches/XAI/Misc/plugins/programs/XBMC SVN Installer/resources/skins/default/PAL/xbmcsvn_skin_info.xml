<window>
    <defaultcontrol>30090</defaultcontrol>
    
	<coordinates>
		<system>1</system>
		<posx>180</posx>
		<posy>35</posy>
	</coordinates>
	
	<controls>
		<!-- ** Background image ** -->
		<control type="image">
			<description>Background Image</description>
			<posx>-49</posx>
			<posy>13</posy>
			<width>440</width>
			<height>495</height>
			<texture>dialog-panel.png</texture>
		</control>
		
		<!-- ** Name ** -->
		<control type="label" id="30010">
			<description>Skin name</description>
			<posx>-25</posx>
			<posy>30</posy>
			<width>350</width>
			<align>left</align>
			<font>special13</font>
            <textcolor>FFFFFFFF</textcolor>
		</control>
		
		<!--  ** Rating stars ** -->
		<control type="image" id="30020">
			<description>Rating stars</description>
			<posx>-25</posx>
			<posy>60</posy>
			<width>120</width>
			<height>16</height>
		</control>

		<!-- ** Xbox Compatible (label) ** -->
		<control type="label" id="30030">
			<description>Xbox Compatible (label)</description>
			<posx>280</posx>
			<posy>40</posy>
			<width>100</width>
			<align>left</align>
			<font>font10</font>
            <textcolor>FFFFFFFF</textcolor>
		</control>

		<!-- ** Xbox Compatible (value) ** -->
		<control type="label" id="30040">
			<description>xbox Compatible (value)</description>
			<posx>375</posx>
			<posy>40</posy>
			<width>100</width>
			<align>right</align>
			<font>font10</font>
            <textcolor>FFFFFFFF</textcolor>
		</control>

		<!-- ** Downloads (label) ** -->
		<control type="label" id="30050">
			<description>Downloads (label)</description>
			<posx>280</posx>
			<posy>60</posy>
			<width>100</width>
			<align>left</align>
			<font>font10</font>
            <textcolor>FFFFFFFF</textcolor>
		</control>

		<!-- ** Downloads (value) ** -->
		<control type="label" id="30060">
			<description>Downloads (value)</description>
			<posx>375</posx>
			<posy>60</posy>
			<width>100</width>
			<align>right</align>
			<font>font10</font>
            <textcolor>FFFFFFFF</textcolor>
		</control>
		
		<!-- ** Separator line ** -->
		<control type="image">
			<posx>-35</posx>
			<posy>80</posy>
			<width>420</width>
			<height>10</height>
			<texture>separator.png</texture>
		</control>		
		
		<!-- ** Description ** -->
		<control type="textbox" id="30070">
			<description></description>
			<posx>-25</posx>
			<posy>90</posy>
			<width>405</width>
			<height>40</height>
			<font>font10</font>
			<textcolor>FFFFFFFF</textcolor>
			<pulseonselect>true</pulseonselect>
		</control>
		
		<control type="group" id="30080">
		
			<!--  ** Previous screenshot (button) -->
			<control type="button" id="30081">
				<description>Screenshot - Previous button</description>
				<posx>-5</posx>
				<posy>255</posy>
				<width>16</width>
				<height>32</height>
				<texturefocus>scroll-left-focus.png</texturefocus>
				<texturenofocus>scroll-left.png</texturenofocus>
				<onleft>30084</onleft>
				<onright>30084</onright>
				<onup>30090</onup>
				<ondown>30090</ondown>
			</control>
			
			<!-- ** Screenshot - Please wait... ** -->
			<control type="label" id="30082">
				<description>Screenshot - please wait...</description>
				<posx>120</posx>
				<posy>260</posy>
				<width>120</width>
				<align>center</align>
				<font>font10</font>
	            <textcolor>FFFFFFFF</textcolor>
			</control>		
			
			<!--  ** Screenshot ** -->
			<control type="image" id="30083">
				<description>Screenshot</description>
				<posx>37</posx>
				<posy>140</posy>
				<width>280</width>
				<height>280</height>
				<visible>true</visible>
				<texture></texture>
				<aspectratio>scale</aspectratio>
			</control>		
			
			<!--  ** Next screenshot (button) -->
			<control type="button" id="30084">
				<description>Screenshot - Next button</description>
				<posx>345</posx>
				<posy>255</posy>
				<width>16</width>
				<height>32</height>
				<texturefocus>scroll-right-focus.png</texturefocus>
				<texturenofocus>scroll-right.png</texturenofocus>
				<onleft>30081</onleft>
				<onright>30081</onright>
				<onup>30090</onup>
				<ondown>30090</ondown>
			</control>
			
		</control>
		
		<control type="group" id="30090">
		
			<!-- ** Install button ** -->
			<control type="button" id="30091">
				<description>Install Button</description>
				<posx>80</posx>
				<posy>430</posy>
				<width>100</width>
				<height>30</height>
				<align>center</align>
				<aligny>center</aligny>
				<font>font13</font>
				<textcolor>ffffffff</textcolor>
				<texturefocus>keyboard-btn-space-focus.png</texturefocus>
				<texturenofocus>keyboard-btn-space.png</texturenofocus>
				<disabledcolor>60d0d0d0</disabledcolor>
				<shadowcolor>ff000000</shadowcolor>
				<onup>30080</onup>
				<onleft>30092</onleft>
				<onright>30092</onright>
				<ondown>30080</ondown>
			</control>		
	
			<!-- ** Cancel button ** -->
			<control type="button" id="30092">
				<description>Cancel Button</description>
				<posx>190</posx>
				<posy>430</posy>
				<width>100</width>
				<height>30</height>
				<align>center</align>
				<aligny>center</aligny>
				<font>font13</font>
				<textcolor>ffffffff</textcolor>
				<texturefocus>keyboard-btn-space-focus.png</texturefocus>
				<texturenofocus>keyboard-btn-space.png</texturenofocus>
				<disabledcolor>60d0d0d0</disabledcolor>
				<shadowcolor>ff000000</shadowcolor>
				<onup>30080</onup>
				<onleft>30091</onleft>
				<onright>30091</onright>
				<ondown>30080</ondown>
			</control>
			
		</control>
	</controls>
</window>